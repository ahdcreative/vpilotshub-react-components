{
  "name": "@ahdcreative/react-components",
  "version": "1.0.0",
  "description": "React components library uses by AHDCreative for vPilotsHub Projects",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build:lib": "rollup -c --bundleConfigAsCjs",
    "build:extract": "node extract-selectors.js",
    "build": "npm run build:lib && npm run build:extract",
    "build:watch": "rollup -cw --bundleConfigAsCjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "storybook": "start-storybook -p 6006",
    "storybook:export": "build-storybook",
    "generate": "node ./util/create-component",
    "prepublishOnly": "npm run build"
  },
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "types": "build/index.d.ts",
  "peerDependencies": {
    "@tabler/icons": "^3.1.0",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@ahdcreative/vpilothub-tailwind-config": "^1.0.0",
    "@ahdcreative/vpilotshub-api": "^1.0.3",
    "@babel/core": "^7.24.3",
    "@marfle/react-leaflet-nmscale": "^3.0.0",
    "@react-leaflet/core": "^2.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@storybook/react": "^8.0.4",
    "@tabler/icons": "^3.1.0",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/jest": "^29.5.12",
    "@types/leaflet": "^1.9.8",
    "@types/leaflet.polylinemeasure": "^1.0.4",
    "@types/react": "^18.2.69",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "babel-preset-react-app": "^10.0.1",
    "cssnano": "^6.1.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "leaflet": "^1.9.4",
    "leaflet.polylinemeasure": "^3.0.0",
    "list-selectors": "^2.0.1",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "rollup": "^4.13.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.72.0",
    "sass-loader": "^14.1.1",
    "tailwindcss": "npm:@tailwindcss/postcss@latest",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "colors": "^1.4.0"
  }
